{
  "name": "tweakjs",
  "author":"Blake Newman <blake.newman@ignitiononline.com>",
  "description": "TweakJS MVC Framework",
  "version": "1.3.3",
  "license": "MIT",
  "homepage": "http://tweakjs.com",
  "main": [
    "lib/tweak.js",
    "lib/tweak.view.html.js"
  ],
  "keywords": ["MVC", "Components", "TweakJS", "Application"],
  "ignore": [
    "app",
    "docs",
    "test",
    "public",
    "vendor",
    "bower_components",
    "node_modules",
    ".git",
    ".editorconfig",
    ".floo",
    ".flooignore",
    ".gitignore",
    "package.json"
  ],
  "devDependencies": {
    "jquery": "*",
    "mocha": "*",
    "chai": "*",
    "sinon-chai": "*",
    "sinonjs": "*"
  },
  "overrides": {
    "sinon-chai": {
      "main": "./lib/sinon-chai.js"
    },
    "mocha": {
      "main": [
        "mocha.js",
        "mocha.css"
      ]
    }
  }
}
